{
  "expo": {
    "name": "App Trabajo en Terreno",
    "slug": "app-trabajo-terreno-mobile",
    "version": "1.8.4",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#3b82f6"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apptrabajoenterreno.mobile"
    },
    "android": {
      "package": "com.apptrabajoenterreno.mobile",
      "versionCode": 36,
      "usesCleartextTraffic": true,
      "permissions": [
        "INTERNET",
        "POST_NOTIFICATIONS",
        "ACCESS_NETWORK_STATE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#3b82f6"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": {
            "scheme": "appterreno"
          }
        }
      ]
    },
    "web": {},
    "extra": {
      "eas": {
        "projectId": "00a515c1-6a8f-4c94-9637-f2ca52cf16ca"
      }
    },
    "plugins": [
      "expo-secure-store",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#3b82f6",
          "mode": "production"
        }
      ]
    ],
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#3b82f6",
      "androidMode": "default",
      "androidCollapsedTitle": "App Trabajo en Terreno"
    }
  }
}
